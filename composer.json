{
    "name": "apsonex/font",
    "description": "Font",
    "keywords": [
        "apsonex",
        "font"
    ],
    "homepage": "https://github.com/apsonex/font",
    "license": "MIT",
    "authors": [
        {
            "name": "Apsonex Inc.",
            "email": "info@apsonex.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "illuminate/support": "^12.15",
        "laravel/pint": "^1.0",
        "pestphp/pest": "^3.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Apsonex\\Font\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Apsonex\\Font\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
